# Frontend Environment Variables (copy to .env and adjust as needed)
# IMPORTANT: Do not commit your real .env

# Base API URL to backend REST API. If both are set, REACT_APP_API_BASE takes precedence.
REACT_APP_API_BASE=https://localhost:3001/api
# Alternative backend URL (used if REACT_APP_API_BASE not set)
REACT_APP_BACKEND_URL=https://localhost:3001/api

# Frontend public URL (used for redirects if applicable)
REACT_APP_FRONTEND_URL=https://localhost:3000
# WebSocket base URL (reserved for future realtime features)
REACT_APP_WS_URL=wss://localhost:3001/ws

# Environment name
REACT_APP_NODE_ENV=development
# Disable Next.js telemetry (reserved, has no effect in CRA)
REACT_APP_NEXT_TELEMETRY_DISABLED=1
# Generate source maps for production builds
REACT_APP_ENABLE_SOURCE_MAPS=true

# Port for local dev server (CRA uses PORT env)
REACT_APP_PORT=3000

# Trust proxy setting (reserved for hosting)
REACT_APP_TRUST_PROXY=false

# Logging verbosity
REACT_APP_LOG_LEVEL=info

# Healthcheck path (can be absolute or relative). Default is /api/health.
REACT_APP_HEALTHCHECK_PATH=/api/health

# Feature flags and experiments (JSON or comma separated)
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
